##
. proc 2>/dev/null

function show_menu 
{
	show_banner
	show_selection	

	read -p ">> " opt

	case $opt in
		1)
			set_enum_config
			;;
		2)
			echo null
			show_menu
			;;
		3)
			echo null
			show_menu
			;;
		4)
			echo null
			show_menu
			;;
		5) 
			show_help_screen
			;;
		6)	
			exit 0
			;;
		*)
			show_menu
			;;
	esac
}
